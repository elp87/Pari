        Title="Окно экспорта" Height="330" Width="466" Closing="Window_Closing" ResizeMode="CanMinimize">
    <Grid>
        <Image Height="{Binding}" HorizontalAlignment="Left" Name="image1" Stretch="Fill" VerticalAlignment="Top" Width="{Binding}" Source="/PariTrainer;component/Images/236_5.jpg" Opacity="0.7" />
        <Button Content="Обзор" Height="23" HorizontalAlignment="Left" Margin="357,17,0,0" Name="buttonBrowse" VerticalAlignment="Top" Width="75" Click="buttonBrowse_Click" />
        <Label Height="28" HorizontalAlignment="Left" Margin="12,12,0,0" Name="labelFileName" VerticalAlignment="Top" Foreground="White" />
        <Button Content="Экспорт" Height="23" HorizontalAlignment="Left" Margin="198,218,0,0" Name="buttonExport" VerticalAlignment="Top" Width="75" Click="buttonExport_Click" />
        
    </Grid>
</Window>
